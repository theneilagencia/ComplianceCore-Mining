{
  "version": 2,
  "buildCommand": "pnpm install --frozen-lockfile && pnpm vite build",
  "outputDirectory": "dist/public",
  "installCommand": "echo 'Using buildCommand install'",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://qivo-mining.onrender.com/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

